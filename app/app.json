{
  "expo": {
    "name": "Yaplingo",
    "slug": "yaplingo",
    "version": "0.1.0",
    "owner": "tnychn",
    "scheme": "yaplingo",
    "plugins": [
      "expo-router",
      "expo-audio",
      "expo-font",
      "expo-dev-client",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#00c951",
          "image": "./assets/splash.png",
          "imageWidth": 256
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": false,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "yaplingo",
      "userInterfaceStyle": "automatic",
      "appleTeamId": "R5U6UH4TAF"
    },
    "android": {
      "package": "yaplingo",
      "userInterfaceStyle": "automatic",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    }
  }
}
